const filled = [];
function array_cpy(xs) {
    let res = [];
    let len = array_length(xs);
    for (let i = 0; i < len; i = i + 1) {
        res[i] = [];
        for (let j = 0; j < len; j = j + 1) {
            res[i][j] = xs[i][j];
        }
    }
    return res;
}
