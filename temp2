let move_dir = [0, 0];

// Other functions
const add_vec = (v1, v2) => [v1[0] + v2[0], v1[1] + v2[1]];
const bound_vec = v => [(v[0] + size) % size, (v[1] + size) % size];

let turn_key = false;
let turn_key_flg = false;
function input() {

   if (input_key_down("w")) {
	if(turn_key) {
		move_dir = [1, 0];
	}
	else {
       move_dir = [-1, 0];
	}
       play_audio(move);
   } else if (input_key_down("a")) {
	if(turn_key) {
		move_dir = [0, 1];
	}
	else {
       move_dir = [0, -1];
	}
       play_audio(move);
   } else if (input_key_down("s")) {
	if(turn_key) {
		move_dir = [-1, 0];
	}
	else {
       move_dir = [1, 0];
	}
       play_audio(move);
   } else if (input_key_down("d")) {
       if(turn_key) {
		move_dir = [0, -1];
	}
	else {
       move_dir = [0, 1];
	}
       play_audio(move);
   }
}
let alive = true;

